includes:
    - themes/mozilla-builders/vendor/szepeviktor/phpstan-wordpress/extension.neon
parameters:
    level: max
    inferPrivatePropertyTypeFromConstructor: true
    # TODO Add types inside arrays
    checkMissingIterableValueType: false
    bootstrapFiles:
        - tests/phpstan/bootstrap.php
    scanFiles:
        - themes/mozilla-builders/vendor/php-stubs/acf-pro-stubs/acf-pro-stubs.php
    paths:
        - themes/mozilla-builders/includes/
    excludePaths:
        - */node_modules/*
